From 36eeb6e95c27b07bf0b5a9af4d805530a79530dd Mon Sep 17 00:00:00 2001
From: Scott Ellis <scott@jumpnowtek.com>
Date: Sun, 5 Jun 2016 16:52:09 -0400
Subject: [PATCH 1/4] overo: Enable SDIO interrupts for Wifi interface

---
 arch/arm/boot/dts/omap3-overo-base.dtsi | 1 +
 1 file changed, 1 insertion(+)

diff --git a/arch/arm/boot/dts/omap3-overo-base.dtsi b/arch/arm/boot/dts/omap3-overo-base.dtsi
index 18e1649..c391f59 100644
--- a/arch/arm/boot/dts/omap3-overo-base.dtsi
+++ b/arch/arm/boot/dts/omap3-overo-base.dtsi
@@ -189,6 +189,7 @@
 	vmmc_aux-supply = <&w3cbw003c_wifi_nreset>;
 	bus-width = <4>;
 	cap-sdio-irq;
+	interrupts-extended = <&intc 86 &omap3_pmx_core 0x12e>; /* gpio_133 (mmc2.dat1) */
 	non-removable;
 };
 
